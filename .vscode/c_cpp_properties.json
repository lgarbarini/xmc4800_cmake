{
    "env": {
      "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${TOOLCHAIN_PREFIX}/${TOOLCHAIN}/include",
            "${XMCLIB_DIR}/CMSIS/Include",
            "${XMCLIB_DIR}/CMSIS/Core/Include",
            "${XMCLIB_DIR}/XMCLib/inc",
            "${XMCLIB_DIR}/CMSIS/Infineon/XMC4800_series/Include"
        ],
      "myCompilerPath": "${TOOLCHAIN_PREFIX}/bin/arm-none-eabi-gcc"
    },
    "configurations": [
      {
        "name": "Linux",
        "intelliSenseMode": "gcc-x64",
        "includePath": ["${myDefaultIncludePath}"],
        "compilerPath": "${myCompilerPath}",
        "cStandard": "c99",
        "compileCommands": "${workspaceFolder}/build/compile_commands.json",
      }
    ],
    "version": 4
  }
  
  